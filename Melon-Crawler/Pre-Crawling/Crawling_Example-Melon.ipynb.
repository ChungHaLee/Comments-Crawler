   "outputs": [],
   "source": [
    "# 불용어 사전 불러오기\n",
    "with open('stop_words.txt', 'r') as file:\n",
    "    stop_word = file.readline()\n",
    "    stop_word = str(stop_word)\n",
    "    \n",
    "stop_word = stop_word.replace(\"\\ufeff\", '').replace(\"'\", '').replace(\",\", '').replace('\\n', '').replace(\"’\", '').replace(\"‘\", '')\n",
    "stop_words = stop_word.split()"
   ]
  },
